* ^concept[4].code = #consent-form
* ^concept[4].display = "Consent form"
* ^concept[4].definition = "The document used to obtain and record consent or assent."
* ^concept[5].code = #consent-plan
* ^concept[5].display = "Consent plan"
* ^concept[5].definition = "The detailed protocol for how consent or assent will be obtained."
* ^concept[6].code = #data-access-plan
* ^concept[6].display = "Data access plan"
