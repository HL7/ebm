* ^concept[11].code = #dissemination-plan
* ^concept[11].display = "Dissemination plan"
* ^concept[11].definition = "The detailed protocol for how research study results will be reported and advertised."
* ^concept[12].code = #education
* ^concept[12].display = "Education"
