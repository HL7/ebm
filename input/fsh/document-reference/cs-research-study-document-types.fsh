* ^concept[8].code = #dataset
* ^concept[8].display = "Dataset"
* ^concept[8].definition = "The collection of data generated by the research study."
* ^concept[9].code = #dataset-access
* ^concept[9].display = "Dataset access"
* ^concept[9].definition = "The information related to how to access the dataset."
* ^concept[10].code = #declaration-of-interests
* ^concept[10].display = "Declaration of interests"
